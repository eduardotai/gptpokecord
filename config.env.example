# Copie este arquivo para .env e preencha com seus valores locais. NÃO COMMITAR .env

# Token do seu bot do Discord
DISCORD_TOKEN=

# ID da aplicação do Discord (Client ID)
CLIENT_ID=

# ID do servidor (opcional)
GUILD_ID=

# ID do canal de comandos (opcional)
COMMAND_CHANNEL_ID=

# ID do canal de spawn automático (opcional; tem prioridade sobre COMMAND_CHANNEL_ID)
SPAWN_CHANNEL_ID=1333923063791751281
# Janelas de spawn (minutos): intervalo aleatório entre mínimo e máximo
SPAWN_MIN_MINUTES=5
SPAWN_MAX_MINUTES=20
# Cooldown mínimo entre spawns no mesmo canal (minutos)
SPAWN_COOLDOWN_MINUTES=6

# Admin master (habilita /admin somente para este usuário)
MASTER_USER_ID=290170816027688960
